{
  "name": "mikeill/mz-mindbody-api",
  "description": "Interface with Mindbody Online API",
  "keywords": [
    "MBO",
    "mindbody",
    "jquery",
    "wordpress",
    "plugin"
  ],
  "homepage": "https://github.com/MikeiLL/mz-mindbody-api",
  "license": "GPL2",
  "authors": [
    {
      "name": "Mike iLL Kilmer",
      "email": "mike@mzoo.org",
      "homepage": "http://mzoo.org",
      "role": "Developer"
    }
   ],
   "type": "wordpress-plugin",
   "require": {
     "php": ">=7.1",
     "composer/installers": "~1.0",
     "squizlabs/php_codesniffer": "*",
     "wp-coding-standards/wpcs": "*"
   },
   "autoload": {
     "psr-4": {
       "MzMindbody\\": "inc"
     }
   },
   "config": {
     "optimize-autoloader": true,
     "preferred-install": "dist",
     "sort-packages": true
   },
   "scripts": {
     "lint": [
       "phpcs --extensions=php --standard=PSR12 src *.php --ignore=src/libraries/Gamajo-Template-Loader.php src/libraries/rarst"
     ],
     "fix": ["phpcbf --extensions=php --standard=PSR12 src *.php --ignore=src/libraries/Gamajo-Template-Loader.php src/libraries/rarst/*"]
   }
}
